{
    "builds": [
        {
            "src": "firstproject/wsgi.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "15mb", 
                "runtime": "python3.9",
                "entrypoint": "firstproject/wsgi.py"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "firstproject/wsgi.py"
        }
    ]
}
